// code that executes before the loop begins
let count = 1;

// condition that must be true for the loop to keep executing
while (count <= 10) {
  console.log("did something difficult | while loop");
  // code that runs at the bottom of each iteration
  count = count + 1;
}
// for(part1; part2; part3){

// }

for (let count = 1; count <= 10; count = count + 1) {
  console.log("did something diffcult | for loop");
}
